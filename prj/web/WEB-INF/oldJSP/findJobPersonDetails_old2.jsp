<%@ page import="qin.javaee8.hibernate.domain.Gender" %><%@ page import="java.util.Iterator" %><%@ page import="java.util.List" %><%@ taglib prefix="c" uri="http://java.sun.com/jstl/core_rt" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><head>    <jsp:include page="/WEB-INF/include_easyui.jsp"></jsp:include>    <link rel="stylesheet" href="<c:url value="/qinCSS/qin.findJobPersonDetails.css"/> ">    <script type="text/javascript" src="<c:url value="/qinJs/findJobPerson/qin.findJobPersonDetails.js"/> "></script></head><body class="easyui-layout"><div data-options="region:'west',title:'导航菜单',split:true" style="width:200px;">    <jsp:include page="/menuJSP/menuIndex.jsp"></jsp:include></div><div data-options="region:'center',title:'主页'" style="padding:5px;background:#eee;height: 3000px;">    <div class="easyui-panel" title="基本信息"         style="width:100%;height:300px;padding:10px;background:#fafafa;"         data-options="iconCls:'icon-help',closable:true, collapsible:true,maximizable:true, collapsed:false">        一共有        <font class="countPerson">            <%                final String end = ",&nbsp;";                List<Object[]> objList_basic = (List<Object[]>) request.getAttribute("findJobPersonInfo");                //招聘人员姓名                StringBuilder jobPersonName = new StringBuilder();                //招聘人员性别                StringBuilder jobPersonGender = new StringBuilder();                //招聘人员以前公司名称                StringBuilder jobPersonOldCompanyName = new StringBuilder();                //招聘人员以前工作                StringBuilder jobPersonOldJob = new StringBuilder();                //招聘人员开始工作时间                StringBuilder jobPersonStartJobTime = new StringBuilder();                //招聘人员职位名称                StringBuilder jobPersonJobName = new StringBuilder();                //招聘人员现在城市                StringBuilder jobPersonNowCity = new StringBuilder();                //招聘人员出生年月                StringBuilder jobPersonBirth = new StringBuilder();                //招聘人员是否结婚                StringBuilder jobPersonIsMarried = new StringBuilder();                //招聘人员手机                StringBuilder jobPersonMobile = new StringBuilder();                //招聘人员邮箱                StringBuilder jobPersonEmail = new StringBuilder();                //招聘人员国籍                StringBuilder jobPersonCountry = new StringBuilder();                //招聘人员户口                StringBuilder jobPersonLocation = new StringBuilder();                for (Iterator<Object[]> oIt = objList_basic.iterator(); oIt.hasNext(); )                {                    Object[] objects = oIt.next();                    jobPersonName.append(objects[0]).append(end);                    //判断性别                    Gender gender = (Gender) objects[1];                    if (gender.equals(Gender.GIRL)) jobPersonGender.append("女").append(end);                    if (gender.equals(Gender.BOY)) jobPersonGender.append("男").append(end);                    jobPersonGender.append(jobPersonGender);                    jobPersonOldCompanyName.append(objects[2]).append(end);                    jobPersonOldJob.append(objects[3]).append(end);                    jobPersonStartJobTime.append(objects[4].toString()).append(end);                    jobPersonJobName.append(objects[5]).append(end);                    jobPersonNowCity.append(objects[6]).append(end);                    jobPersonBirth.append(objects[7].toString()).append(end);                    jobPersonIsMarried.append(objects[8]).append(end);                    jobPersonMobile.append(objects[9]).append(end);                    jobPersonEmail.append(objects[10]).append(end);                    jobPersonCountry.append(objects[11]).append(end);                    jobPersonLocation.append(objects[12]).append(end);                }                //jobPersonNameReturn(返回的结果字符串)                String jobPersonNameR = jobPersonName.substring(0, getLastIndexOf(jobPersonName));                String jobPersonGenderR = jobPersonGender.substring(0, getLastIndexOf(jobPersonGender));                String jobPersonOldCompanyNameR = jobPersonOldCompanyName                        .substring(0, getLastIndexOf(jobPersonOldCompanyName));                String jobPersonOldJobR = jobPersonOldJob.substring(0, getLastIndexOf(jobPersonOldJob));                String jobPersonStartJobTimeR = jobPersonStartJobTime                        .substring(0, getLastIndexOf(jobPersonStartJobTime));                String jobPersonJobNameR = jobPersonJobName                        .substring(0, getLastIndexOf(jobPersonJobName));                String jobPersonNowCityR = jobPersonNowCity                        .substring(0, getLastIndexOf(jobPersonNowCity));                String jobPersonBirthR = jobPersonBirth                        .substring(0, getLastIndexOf(jobPersonBirth));                String jobPersonIsMarriedR = jobPersonIsMarried                        .substring(0, getLastIndexOf(jobPersonIsMarried));                String jobPersonMobileR = jobPersonMobile                        .substring(0, getLastIndexOf(jobPersonMobile));                String jobPersonEmailR = jobPersonEmail                        .substring(0, getLastIndexOf(jobPersonEmail));                String jobPersonCountryR = jobPersonCountry                        .substring(0, getLastIndexOf(jobPersonCountry));                String jobPersonLocationR = jobPersonLocation                        .substring(0, getLastIndexOf(jobPersonLocation));            %>            <% out.println(objList_basic.size()); %>        </font>条记录:<br>        <font class="basicInfoDiv">招聘人员姓名:</font>        <font class="basicInfoDetails"><% out.println(jobPersonNameR); %></font><br>        <font class="basicInfoDiv">招聘人员性别:</font>        <font class="basicInfoDetails"><% out.println(jobPersonGenderR); %></font><br>        <font class="basicInfoDiv">招聘人员以前公司名称:</font>        <font class="basicInfoDetails"><% out.println(jobPersonOldCompanyNameR); %></font><br>        <font class="basicInfoDiv">招聘人员以前工作:</font>        <font class="basicInfoDetails"><% out.println(jobPersonOldJobR); %></font><br>        <font class="basicInfoDiv">招聘人员开始工作时间:</font>        <font class="basicInfoDetails"><% out.println(jobPersonStartJobTimeR); %></font><br>        <font class="basicInfoDiv">招聘人员职位名称:</font>        <font class="basicInfoDetails"><% out.println(jobPersonJobNameR); %></font><br>        <font class="basicInfoDiv">招聘人员现在城市:</font>        <font class="basicInfoDetails"><% out.println(jobPersonNowCityR);%></font><br>        <font class="basicInfoDiv">招聘人员出生年月:</font>        <font class="basicInfoDetails"><% out.println(jobPersonBirthR); %></font><br>        <font class="basicInfoDiv">招聘人员是否结婚:</font>        <font class="basicInfoDetails"><% out.println(jobPersonIsMarriedR); %></font><br>        <font class="basicInfoDiv">招聘人员手机:</font>        <font class="basicInfoDetails"><% out.println(jobPersonMobileR); %></font><br>        <font class="basicInfoDiv">招聘人员邮箱:</font>        <font class="basicInfoDetails"><% out.println(jobPersonEmailR); %></font><br>        <font class="basicInfoDiv">招聘人员国籍:</font>        <font class="basicInfoDetails"><% out.println(jobPersonCountryR); %></font><br>        <font class="basicInfoDiv">招聘人员户口:</font>        <font class="basicInfoDetails"><% out.println(jobPersonLocationR); %></font><br>    </div>    <br>    <div class="easyui-panel" title="职业意向"         style="width:100%;height:300px;padding:10px;background:#fafafa;"         data-options="iconCls:'icon-save',closable:true, collapsible:true,maximizable:true, collapsed:true">        <!--                期望行业    期望职能    期望地点    期望年薪    目前年薪        -->        <table id="tb_findJobWants"></table>    </div>    <br>    <div class="easyui-panel" title="工作经历"         style="width:100%;height:30