<%@ page contentType="text/html;charset=UTF-8" language="java" %><!-- 头版信息 --><%--页面字段	对应实体类字段	字段控件类型	字段说明姓名	jobPersonName	text	不能为空性别	pageJobPersonGender	radio	不能为空公司名称	jobPersonOldCompanyName	text	智能提示当前行业职能	jobPersonOldJob	dialog	从对话框中选取开始工作年份	pageJobPersonStartJobTime	select	不能为空职位名称	jobPersonJobName	text	智能提示当前城市	jobPersonNowCity	dialog	从对话框中选取--%><form id="frm_addBasic" name="frm_addBasic" method="post">    <table width="100%;" style="margin-right:5%;" border="0" id="tb_addBasic">        <tr align="center">            <td>页面字段</td>            <td>字段控件</td>        </tr>        <tr align="center">            <td>姓名</td>            <td>                <input type="text" class="form-control" placeholder="招聘人员姓名"                       id="jobPersonName" name="jobPersonName">            </td>        </tr>        <tr align="center">            <td>性别</td>            <td>                <select class="easyui-combobox" id="pageJobPersonGender" name="pageJobPersonGender"                        style="width: 90%;">                    <option>男</option>                    <option>女</option>                    <option>保密</option>                </select>            </td>        </tr>        <tr align="center">            <td>公司名称</td>            <td>                <input type="text" id="jobPersonOldCompanyName" name="jobPersonOldCompanyName"                       placeholder="公司名称">                <img src="/jsLib/easyui15/themes/icons/search.png"                     id="img_image" name="img_image">            </td>        </tr>        <tr align="center">            <td>当前行业职能</td>            <td>                <input type="text" id="jobPersonOldJob" name="jobPersonOldJob"                       placeholder="当前行业职能">                <img src="/jsLib/easyui15/themes/icons/search.png"                     id="img_oldJob" name="img_oldJob">            </td>        </tr>        <tr align="center">            <td>开始工作年份</td>            <td>                <select id="pageJobPersonStartJobTime" class="easyui-combobox"                        name="pageJobPersonStartJobTime" style="width:300px;">                    <%                        for (int i = 1914; i <= 2016; i++)                        {                            out.println("<option>" + i + "</options>");                        }                    %>                </select>            </td>        </tr>        <tr align="center">            <td>职位名称</td>            <td>                <input type="text" id="jobPersonJobName" name="jobPersonJobName"                       placeholder="职位名称">                <img src="/jsLib/easyui15/themes/icons/search.png"                     id="img_jobName" name="img_jobName">            </td>        </tr>        <tr align="center">            <td>当前城市</td>            <td>                <input type="text" id="jobPersonNowCity" name="jobPersonNowCity"                       placeholder="当前城市">                <img src="/jsLib/easyui15/themes/icons/search.png"                     id="img_oldCity" name="img_oldCity">            </td>        </tr>    </table></form>